<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://jqassistant.github.io/jqassistant/current/schema/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant jMolecules Plug-In" id="jqa.plugin.jmolecules"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.jmolecules.descriptor.JMoleculesDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.architecture.ArchitectureDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.architecture.layered.LayeredDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.architecture.layered.LayerDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.architecture.onion.OnionDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.architecture.onion.RingDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.ddd.BoundedContextDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.ddd.DDDDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.ddd.ModuleDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.stereotype.StereotypesFileDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.stereotype.StereotypeDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.stereotype.StereotypeAssignmentDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.descriptor.stereotype.StereotypeGroupDescriptor</class>
        <class>org.jqassistant.plugin.jmolecules.repository.StereotypeRepository</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.jmolecules.scanner.stereotype.StereotypesFileScanner</class>
    </scanner>
    <rules>
        <resource>jmolecules-cqrs.xml</resource>
        <resource>jmolecules-ddd.xml</resource>
        <resource>jmolecules-ddd-strict.xml</resource>
        <resource>jmolecules-event.xml</resource>
        <resource>jmolecules-hexagonal.xml</resource>
        <resource>jmolecules-hexagonal-strict.xml</resource>
        <resource>jmolecules-layered.xml</resource>
        <resource>jmolecules-layered-strict.xml</resource>
        <resource>jmolecules-onion-simplified.xml</resource>
        <resource>jmolecules-onion-simplified-strict.xml</resource>
        <resource>jmolecules-onion-classical.xml</resource>
        <resource>jmolecules-onion-classical-strict.xml</resource>
    </rules>
</jqassistant-plugin>
